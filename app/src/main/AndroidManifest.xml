<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.todd.trackitcoach" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".IntroActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Coach_Main_Page"
            android:label="@string/title_activity_coach__main__page" >
        </activity>
        <activity
            android:name=".Review_Page"
            android:label="@string/title_activity_review__page" >
        </activity>
        <activity
            android:name=".RunLongActivity"
            android:label="@string/title_activity_run_long" >
        </activity>
        <activity
            android:name=".RunEventActivity"
            android:label="@string/title_activity_run_event" >
        </activity>
    </application>

</manifest>
